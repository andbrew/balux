,EAkBA;AACA,WAAA,UAAA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,KAAA,EAAA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,OAAA,EAAA,IAFA;AAGA,QAAA,aAAA,EAAA,IAHA;AAIA,QAAA,WAAA,EAAA;AAJA,OAAA;AAMA;AA1BA;AAZA,CAAA","sourcesContent":["<template>\r\n  <div class=\"MovieAddComponent\">\r\n    <div class=\"container\">\r\n      <form action=\"\" class=\"form-horizontal\">\r\n        <div class=\"form-group left\"> <br>\r\n          <label for=\"\" class=\"control-label col-sm-2\">Nombre de la película</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              name=\"title\"\r\n              id=\"title\"\r\n              v-model=\"form.name\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group left\">\r\n          <label for=\"\" class=\"control-label col-sm\">Año</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              name=\"description\"\r\n              id=\"description\"\r\n              v-model=\"form.year\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group left\">\r\n          <label for=\"\" class=\"control-label col-sm\">ID</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              name=\"description\"\r\n              id=\"description\"\r\n              v-model=\"form.id\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <button type=\"button\" class=\"btn btn-primary\" v-on:click=\"guardar()\">\r\n            Guardar\r\n          </button>\r\n          <button type=\"button\" class=\"btn btn-dark margen\" v-on:click=\"salir()\">\r\n            Atrás\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"MovieAddComponent\",\r\n  props: {},\r\n  data: function () {\r\n    return {\r\n      form: {\r\n        name: \"\",\r\n        year: \"\",\r\n        id: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    salir() {\r\n      this.$router.push(\"/movies\");\r\n    },\r\n    guardar() {\r\n      //   this.form.token = localStorage.getItem(\"token\");\r\n      axios\r\n        .post(\"http://localhost:3001/movies/crear\", this.form)\r\n        .then((data) => {\r\n          console.log(data);\r\n          this.makeToast(\"Hecho\", \"Película agregada\", \"success\");\r\n          this.$router.push(\"/movies\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          this.makeToast(\"Error\", \"Error al guardar\", \"error\");\r\n        });\r\n    },\r\n    makeToast(titulo, texto, tipo) {\r\n      this.toastCount++;\r\n      this.$bvToast.toast(texto, {\r\n        title: titulo,\r\n        variant: tipo,\r\n        autoHideDelay: 5000,\r\n        appendToast: true,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.left {\r\n  text-align: left;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}